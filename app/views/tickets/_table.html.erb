<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Assignee</th>
      <th>Status</th>
      <th>Tags</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= link_to ticket.title, ticket %></td>
        <td>user_name</td>
        <td><%= ticket.status %></td>
        <td>tags</td>
        <td>
          <%= link_to 'Edit', edit_ticket_path(ticket) %>
           |
          <%= link_to 'Delete', ticket, method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
